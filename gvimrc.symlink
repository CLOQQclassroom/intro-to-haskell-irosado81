" use gui tabs
  set guioptions+=e

" kill the menubar
  set guioptions-=T

" kill the scrollbars
  set guioptions-=r
  set guioptions-=L

" use a big, pretty font
  set guifont=Inconsolata\ for\ Powerline:h16

" initial window size
  set lines=45 columns=120

" set colorscheme
  colorscheme base16-twilight
