with(@discounts)\n  end\n\n  def search\n    search = search_with_query(params[:query], :shop_id => shop.id, :sort => params[:order], :page => current_page, :page_size => page_limit)\n    @discounts = search.records\n    create_search_paginator(@discounts, search)\n\n    respond_with(@discounts, :root => 'discounts')\n  end\n\n  def show\n    @discount = shop.discounts.find(params[:id])\n    respond_with(@discount)\n  end\n\n  def create\n    binding.pry\n    @discount = build_discount\n    @discount.save unless @discount.errors.present?\n\n    respond_with(@discount, :location => [:admin, @discount.becomes(Discount)]) do |format|\n      format.html do\n        if @discount.errors.blank?\n          redirect_to admin_discounts_path, notice: 'Discount was created', newly_created: true\n        else\n          render action: :new\n        end\n      end\n    end\n  end\n\n  def disable\n    @discount = shop.discounts.find(params[:id])\n    @discount.disable\n\n    respond_with(@discount, :location => [:admin, @discount.becomes(Discount)]) do |format|\n      format.html {redirect_to :back, notice: \"Discount '#{@discount.code}' disabled.\"}\n    end\n  end\n\n  def enable\n    @discount = shop.discounts.find(params[:id])\n    @discount.enable\n\n    respond_with(@discount, :location => [:admin, @discount.becomes(Discount)]) do |format|\n      format.html {redirect_to :back, notice: \"Discount '#{@discount.code}' enabled.\"}\n    end\n  end\n\n  def destroy\n    @discount = shop.discounts.find(params[:id])\n    @discount.destroy\n\n    respond_with(@discount) do |format|\n      format.html {redirect_to :back, notice: \"Discount '#{@discount.code}' deleted.\"}\n    end\n  end\n\n  def set\n    set = DiscountSet.new(shop, bulk_operation_ids)\n    if set.modify_with(params[:operation], :user_id => current_user.id)\n      flash[:notice] = set.message\n      select_clear\n    else\n      flash[:error] = set.message\n    end\n\n    respond_with(set, :root => false)\n  end\n\n 